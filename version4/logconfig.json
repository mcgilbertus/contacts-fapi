{
  "version": 1,
  "formatters": {
    "default": {
      "format": "%(asctime)s-%(levelname)s %(name)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "formatter": "default",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout",
      "level": "DEBUG"
    }
  },
  "root": {
    "handlers": [
      "console"
    ],
    "level": "DEBUG"
  },
  "loggers": {
    "gunicorn": {
      "propagate": true
    },
    "uvicorn": {
      "propagate": true
    },
    "uvicorn.access": {
      "propagate": true
    },
    "sqlalchemy": {
      "propagate": false
    }
  }
}
